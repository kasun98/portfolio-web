{
    "version": 2,
    "builds": [
      {
        "src": "backportfolio/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "backportfolio/wsgi.py"
      }
    ],
    "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
  }
  